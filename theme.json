      "menu-bar-sticky-height": "94px",
      "border-radius-large": "50px",
      "border-radius": "14px",
      "border-radius-small": "7px",
      "form-radius": "99em",
      "button-radius": "99em"
    },
    "color": {
      "palette": [
        {
          "color": "#FFC629",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#003764",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#5271FF",
          "name": "Third",
          "slug": "third"
        },
        {
          "color": "#F2F1E8",
          "name": "Fourth",
          "slug": "fourth"
        },
        {
          "color": "#666666",
          "name": "Fifth",
          "slug": "fifth"
        },
        {
          "color": "#8D021F",
          "name": "Sixth",
          "slug": "sixth"
        },
        {
          "color": "#E44844",
          "name": "Seventh",
          "slug": "seventh"
        },
        {
          "color": "#FF8444",
          "name": "Eighth",
