            {
              "fontFamily": "Source Serif Pro",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "200 900",
              "src": [
                "file:./assets/fonts/source-serif-pro/SourceSerif4Variable-Italic.ttf.woff2"
              ]
            }
          ],
          "fontFamily": "\"Source Serif Pro\", serif",
          "name": "Source Serif Pro",
          "slug": "source-serif-pro"
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "min": "0.7rem",
            "max": "0.875rem"
          },
          "size": "0.875rem",
          "slug": "small"
        },
        {
          "fluid": {
            "min": "0.875rem",
            "max": "1rem"
          },
          "size": "1rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          },
          "size": "1.125rem",
          "slug": "large"
        },
        {
          "fluid": {
            "min": "1.75rem",
            "max": "1.875rem"
          },
          "size": "1.875rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "min": "1.875rem",
            "max": "2.5rem"
          },
          "size": "2.5rem",
          "slug": "xx-large"
        }
      ]
