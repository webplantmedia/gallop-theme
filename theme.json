              "src": [
                "file:./assets/fonts/ibm-plex-mono/IBMPlexMono-Light.woff2"
              ]
            },
            {
              "fontDisplay": "block",
              "fontFamily": "IBM Plex Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/ibm-plex-mono/IBMPlexMono-Regular.woff2"
              ]
            },
            {
              "fontDisplay": "block",
              "fontFamily": "IBM Plex Mono",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/ibm-plex-mono/IBMPlexMono-Italic.woff2"
              ]
            },
            {
              "fontDisplay": "block",
              "fontFamily": "IBM Plex Mono",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/ibm-plex-mono/IBMPlexMono-Bold.woff2"
              ]
            }
          ],
          "fontFamily": "'IBM Plex Mono', monospace",
          "name": "IBM Plex Mono",
          "slug": "ibm-plex-mono"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "200 900",
