  "version": 2,
  "customTemplates": [
    {
      "name": "blank",
      "postTypes": ["page", "post"],
      "title": "Blank"
    },
    {
      "name": "blog-alternative",
      "postTypes": ["page"],
      "title": "Blog (Alternative)"
