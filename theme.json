            ":focus": {
              "typography": {
                "textDecoration": "underline dashed"
              }
            },
            ":active": {
              "typography": {
                "textDecoration": "none"
              }
            },
            "typography": {
              "fontFamily": "var(--wp--preset--font-family--catamaran)",
              "fontSize": "var(--wp--preset--font-size--medium)",
              "fontWeight": "normal",
